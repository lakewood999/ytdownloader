#!/bin/bash

for ruby_env_var in RUBYOPT \ 
                    BUNDLE_BIN_PATH \
                    BUNDLE_GEMFILE \
                    GEM_PATH \
                    GEM_ROOT \
                    GEM_HOME
do
    unset $ruby_env_var
done

export SVWAIT=30

<%= embedded_bin %>/omnibus-ctl ytdownloader <%= embedded_service %>/omnibus-ctl $@