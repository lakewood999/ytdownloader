{
  "compilerOptions": {
    "outDir": "./ytdownloader/static/js",
    "module": "es2015",
    "target": "es2015",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "./ytdownloader/react_src",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "downlevelIteration": true
  },
  "include": ["./ytdownloader/react_src/**/*"],
  "exclude": ["node_modules", "dist"],
  "types": ["typePatches"]
}
